doctype html
html
  head
    include _head
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body.noscroll
    noscript
      div
        strong For full functionality of this site it is necessary to enable JavaScript. Download the update here 
        a(href="http://enable-javascript.com")
          | enable-javascript.com
      
    //if lte IE 8
      p.off-js
        | Ваш браузер устарел, пожалуйста 
        b обновите
        |  его.
    // PRELOADER
    include _preloader
    // PRELOADER
    // wrapper start
    .wrapper
      // header SECTION
      include _header
      // header SECTION
      // sidebar
      include _sidebar
      // sidebar
      // main
      main.main-container
        .heaing-section Products
        .main-container-scroll.view-prop-box
          .sidebar-level.sidebar-level--1
            .sidebar-level-over
              ul
                li
                  a(href="#!").waves-effect.waves-dark Zorg in zorg
                li.active
                  a(href="#!").waves-effect.waves-dark.with-under-level Wankel Evenwicht
              a(href="#!").add-item-lnk.waves-effect.waves-dark ADD PRODUCT
          .sidebar-level.sidebar-level--2
            .sidebar-level-over
              ol
                li.active
                  a(href="#!").waves-effect.waves-dark.with-under-level  Dementie en het  Dementie en het
                li
                  a(href="#!").waves-effect.waves-dark Normen en waar
                li
                  a(href="#!").waves-effect.waves-dark Wat is verplegen
                li
                  a(href="#!").waves-effect.waves-dark Wat is wonen
                li
                  a(href="#!").waves-effect.waves-dark Niet medisch mo
                li
                  a(href="#!").waves-effect.waves-dark Anticiperend rou
                li
                  a(href="#!").waves-effect.waves-dark Wettelijke regelin
              a(href="#!").add-item-lnk.waves-effect.waves-dark ADD MODULE
          .main-container-box
            .container-box
              .add-module
                .container-box-head.clearfix
                  .container-box-head__title
                    .input-field
                      input#inp1(type='text')
                      label(for='inp1') Title of Module
                  .container-box-head__button
                    a.properties.btn.waves-effect.waves-light(href="#!") PROPERTIES
                .container-box-body
                  .list-item-container
                    mixin list-item(title,icon,id)
                      .list-item
                        .list-item-drag
                        .list-item-check
                          input.filled-in(type='checkbox',id="check#{id}")
                          label(for='check#{id}')
                        .list-item-title
                          a(href="#!")= title
                        .list-item-icon
                          span
                            img(src="images/#{icon}.png", alt="")
                    +list-item('Inleiding','icon11','1')
                    +list-item('Doelen','icon12','2')
                    +list-item('Hoe werkt het?','icon10','3')
                    +list-item('Dementie','icon10','4')
                  a(href="#!").add-item
                    span.btn.waves-effect.waves-light
                      img(src="images/icon9.png", alt="")
                    span.btn-txt ADD ITEM
          .back-nav
            span.waves-effect.waves-dark
              | &larr;
          .properties-container.properties-container--first
            .properties-box.timelines-box
              .timelines-select.add-item-bottom
                span.label-tlt Timeline
                select
                  option(value='', disabled='', selected='') Timeline 3
                  option(value='1') Timeline 1
                  option(value='2') Timeline 2
                  option(value='3') Timeline 3
                  option(value='4') Timeline 4
              .input-field
                span Start date
                input(type="date",class="datepicker",value="dd/mm/yyyy",placeholder="dd/mm/yyyy")
              .input-field
                input#inp132(type='time',value="09:00")
                label.active(for='inp132') Time
              .input-field
                span End date
                input(type="date",class="datepicker",value="dd/mm/yyyy",placeholder="dd/mm/yyyy")
              .module-timelines
                mixin module(title,count)
                  .module-time-item
                    .module-time-item__left
                      .module-time__count Module 
                        span=count
                      .module-time__title= title
                    .module-time-item__right
                      span.icon-time
                        img(src="images/time.png", alt="")
                      span Friday 12 - 3 - 2016 - 09:00 
                +module('Dementie en het wankel evenwicht','1')
                +module('Normen en waarden','2')
                +module('Wat is verplegen','3')
                +module('Wat is wonen','4')
                +module('Het medisch model','5')
                +module('Anticiperend rouwen','6')
                +module('Wettelijke regelingen','7')
      // main
      .above-footer
    include _footer
